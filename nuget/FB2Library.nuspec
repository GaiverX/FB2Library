<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>FB2Library</id>
    <version>1.3.0</version>
    <title>Cross-Platform .NET library for read .FB2 ebook files</title>
    <authors>lordkiron,Yauheni Pakala</authors>
    <owners>Yauheni Pakala</owners>
    <licenseUrl>https://github.com/wcoder/FB2Library/blob/master/LICENSE.md</licenseUrl>
    <projectUrl>https://github.com/wcoder/FB2Library</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/wcoder/FB2Library/master/nuget/icon.png</iconUrl>
